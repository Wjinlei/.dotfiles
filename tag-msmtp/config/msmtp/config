account freebufer.wang@gmail.com
host smtp.gmail.com
port 465
from freebufer.wang@gmail.com
user freebufer.wang@gmail.com
passwordeval "pass freebufer.wang@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/w/.config/msmtp/msmtp.log
tls_starttls off

